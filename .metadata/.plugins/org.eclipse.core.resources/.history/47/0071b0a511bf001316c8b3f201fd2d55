package gala.cipher;
/**
* Interface
* @author Mateusz Gala
* @version 2014-04-07
*
*/
public interface Cipher {

	/**
	 * Methode zum verschlüsseln mit dem dazugehörigen Alphabet
	 * @param text eingabetext
	 * @return text verschlüsselter Text
	 */
	public String encrypt(String text);
	
	
	
	public String decrypt(String text);

}