
import gala.cipher.MonoAlphabeticCipher;

import org.junit.Test;

public class test {
	MonoAlphabeticCipher s = new MonoAlphabeticCipher();
	
	
	
	@Test
	public void getSecretAlphabet(){
		s.getSecretAlphabet();
	}
	@Test
	public void encrypt(){
		s.encrypt("Hallo wie gehts");
	}
	@Test
	public void dencrypt(){
		s.decrypt(s.encrypt("Hallo wie gehts"));
				}
	}
}
